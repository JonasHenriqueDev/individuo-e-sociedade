[sub_resource type="GDScript" id="GDScript_ks7rh"]
resource_name = "VideoPlay"
script/source = "extends Control

@onready var video_player = $DominoVideo
@onready var botao_invisivel = $Play


func _ready():
	video_player.autoplay = false
	video_player.stop()
	video_player.stream_position = 0.0


func _on_play_pressed():
	if not video_player.is_playing():
		video_player.play()
	
	botao_invisivel.hide()
"

